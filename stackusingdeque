from collections import deque


class Stack:

    def __init__(self):
        self.__list = deque()

    def is_empty(self):
        return len(self.__list) == 0

    def push(self, i):
        return self.__list.appendleft(i)

    def pop(self):
        return self.__list.popleft()

    def top(self):
        return self.__list[0]

    def traverse(self):
        return self.__list


mystack = Stack()

